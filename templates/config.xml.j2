<!-- {{ ansible_managed }} -->
<config>
  <user v='{{ folding_at_home_user }}'/>
  <team v='{{ folding_at_home_team }}'/>
  <passkey v='{{ folding_at_home_passkey }}'/>
  <power v='{{ folding_at_home_power }}'/>
  <gpu v='{{ folding_at_home_use_gpu }}'/>
  <password v='{{ folding_at_home_password }}' />
{% if folding_at_home_proxy is defined %}
  <proxy v='{{ folding_at_home_proxy }}'/>
  <proxy-enable v='true'/>
{% endif %}
{% if folding_at_home_remote_ips is defined %}
  <allow v='{{ folding_at_home_remote_ips }}' />
{% endif %}
</config>
